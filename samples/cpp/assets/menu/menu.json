        {
            "do": "GoTo",
            "key": "A",
            "name": "Authorization",
            "value": {
                "id": "authorization",
                "item": [
                    {
                        "do": "notify/onStartCBLAuthorization",
                        "key": "1",
                        "name": "Start CBL Authorization",
                        "when" : "!CBLAuthorizationActive && !CBLAuthorizationInProgress"
                    },
                    {
                        "do": "notify/onCancelCBLAuthorization",
                        "key": "1",
                        "name": "Cancel CBL Authorization",
                        "when" : "CBLAuthorizationInProgress && !CBLAuthorizationActive"
                    },
                    {
                        "do": "notify/onLogoutCBLAuthorization",
                        "key": "1",
                        "name": "Logout CBL Authorization",
                        "when" : "CBLAuthorizationActive"
                    },
                    {
                        "do": "notify/onStartAuthProviderAuthorization",
                        "key": "2",
                        "name": "Start Auth Provider Authorization",
                        "when" : "AuthProviderSupported && !AuthProviderAuthorizationActive && !AuthProviderAuthorizationInProgress"
                    },
                    {
                        "do": "notify/onCancelAuthProviderAuthorization",
                        "key": "2",
                        "name": "Cancel Auth Provider Authorization",
                        "when" : "AuthProviderSupported && AuthProviderAuthorizationInProgress && !AuthProviderAuthorizationActive"
                    },
                    {
                        "do": "notify/onLogoutAuthProviderAuthorization",
                        "key": "2",
                        "name": "Logout Auth Provider Authorization",
                        "when" : "AuthProviderSupported && AuthProviderAuthorizationActive"
                    },
                    {
                        "do": "GoBack",
                        "key": "esc",
                        "name": "Go back"
                    }                   
                ],
                "name": "Authorization Menu"
            }
        },
